	width:968px;
	padding:10px 14px 9px 14px;
}
#content-middle .white2-panel {
	width:960px;
}
#footer {
	width: 1008px;
	margin: 0 auto 0;
	height: 61px;
	background:#FFF url(/img/cont-shadow.png) repeat-y 0 0;
}
