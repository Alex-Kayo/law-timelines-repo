/* IDS & clases - MAIN */
.content-left0 {
	float: left;
	width: 53%;
}
.content-right0 {
	margin: 0 0 0 51%;
	width: 49%;
}
#wrapper {
	width: 1008px;
	margin: 0 auto;
	min-height: 100%;
	height: auto !important;
	height: 100%;
	background:#FFF url(/img/cont-shadow1.png) repeat-y 0 0;
	position:relative;
}
#header {
	height: 200px;
	margin-bottom:5px;
}
#header:after {
	content: '.';
	display: block;
	clear: both;
	visibility: hidden;
	height: 0;
}
#middle {
	width: 100%;
	padding: 0 0 26px;
	height: 1%;
	clear:both;
}
#middle:after {
	content: '.';
	display: block;
	clear: both;
	visibility: hidden;
	height: 0;
}
#container {
	width: 100%;
	float: left;
	overflow: hidden;
}
#content-left {
	padding: 0 612px 0 9px;
}
#content-right {
	float: left;
	margin-right: -3px;
	width: 610px;
	margin-left: -619px;
	position: relative;
}
#content-middle {
	float: left;
	margin-right: -3px;
	width: 988px;
	margin-left: 10px;
	position: relative;
}
#content-middle .panel {
	background: transparent url(/img/panel-middle.png) repeat-y 0 0;
	width:988px;
}
#content-middle .panel1 {
	background: transparent url(/img/panel-middle-top.png) no-repeat 0 0;
	width:988px;
}
#content-middle .panel2 {
